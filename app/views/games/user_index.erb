<h1>List of My Games</h1>
<table border = "2">     
<tr><td>Name</td> <td> Game </td> <td> Won = true,  Loss = false </td></tr>
<% @games.each do |game|%>
<tr><td><%= game.user.name %></td>
<td><a href="/games/<%= game.id %>"><%= game.id %></a></td>
<td><%= game.won %></li></td></tr>
<% end %>
</table>
